
public class If { //만약에

	public static void main(String[] args) { //메인메소드 처음 실행
		// TODO Auto-generated method stub
		int charcode =4; //정수 한글자코드 =4
		System.out.println(charcode);//로그출력(한글자코드)
		
		if(charcode ==4) //만약에(한글자코드 ==4-> 4라면)
		{
	      System.out.println("이 숫자는 4다!"); //로그출력
		}
		else { //그밖에
			 System.out.println("이 숫자는 4가 아닙니다!"); //로그출력
		}
}
}
